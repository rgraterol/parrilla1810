@font-face {
    font-family: 'AbrahamLincoln';
    src: url(<%= asset_path 'AbrahamLincoln/AbrahamLincoln.eot' %>);
    src: url(<%= asset_path 'AbrahamLincoln/AbrahamLincoln.eot?iefix' %>) format('eot'),
    url(<%= asset_path 'AbrahamLincoln/AbrahamLincoln.woff' %>) format('woff'),
    url(<%= asset_path 'AbrahamLincoln/AbrahamLincoln.ttf' %>) format('truetype'),
    url(<%= asset_path 'AbrahamLincoln/AbrahamLincoln.svg' %>) format('svg');
    font-weight: normal;
    font-style: normal;
}

